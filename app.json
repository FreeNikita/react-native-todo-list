{
  "expo": {
    "name": "Todo list",
    "slug": "react-native-todo-list",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nikitests.todo-list",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.nikitests.todo_list",
      "versionCode": 1,
    }
  }
}
